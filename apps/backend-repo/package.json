{
  "name": "backend-repo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && pnpm i",
    "dev": "nodemon --exec \"ts-node ./core/app\" --watch ./core --watch ./routes --watch ./config --ext .ts",
    "start": "node -r ./config.js ./dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.16.1",
    "fs-extra": "^11.3.0",
    "http-errors": "^2.0.0",
    "jet-logger": "^2.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/http-errors": "^2.0.4",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22",
    "eslint": "^9",
    "eslint-plugin-n": "^17.15.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^8.23.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
